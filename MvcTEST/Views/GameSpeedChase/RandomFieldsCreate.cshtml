    </div>

    <div id="log"></div>
    <div id="logID"></div>
</body>

<script type="text/javascript">
    var A1 = [];
    var A2 = [];
    var sss;
    if (window.parent.$('#RandomFields').val() != '') {
        A1 = window.parent.$('#RandomFields').val().split(',');
        A2 = window.parent.$('#RandomFieldsID').val().split(',');
        $("p").html(A1.toString());
        $('#log').attr('name', A1.toString());
        $('#logID').attr('name', A2.toString());
    }
    //fancybox-content
    $("[id^='fld']").css('cursor', 'pointer');
    $("#del").css('cursor', 'pointer');

    $("[id^='fld']").click(function () {
        //alert( $('#StartField').html());
        //alert(window.parent.$('#RandomFields').val());
        //sss = $(this).attr('id');

        //if (ss.length == 0) {
        //    ss = ($(this).text());
        //    ssID = (sss.replace('fld', ''));
        //} else {
        //    ss = ss + ',' + ($(this).text());
        //    ssID = ssID + ',' + (sss.replace('fld', ''));
        //}
        A1.push($(this).text());
        A2.push($(this).attr('id').replace('fld', ''));

        $('#log').attr('name', A1.toString());
        $("p").html(A1.toString());
        $('#logID').attr('name', A2.toString());
        //alert(A1 +'-'+A2);
    });
    $("#del").click(function () {
        A1.pop();
        $("p").html(A1.toString());
        A2.pop();
        $('#log').attr('name', A1.toString());
        $('#logID').attr('name', A2.toString());
    });
</script>


</html>
