@model MvcTEST.Models.tblTemplate
@using MvcTEST.HtmlHelpers
@{
    ViewBag.Title = "Start6";
}

    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script type="text/javascript" src="/Scripts/fancybox/jquery.fancybox-1.3.4.js"></script>
    <script type="text/javascript" src="/Scripts//fancybox/jquery.Easing-1.3.pack.js"></script>
    <link id="size-stylesheet" href="@Url.Content("/Content/style.css")" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/Scripts/fancybox/jquery.fancybox-1.3.4.css")" type="text/css" media="screen" />


<div id="l1" style="padding: 10px; width: inherit;position: relative; z-index: 1; -moz-box-shadow: 3px 3px 5px 6px #000; -webkit-box-shadow: 3px 3px 5px 6px #000; box-shadow: 0px 0px 20px 0px gray; border: 0px solid; border-radius: 10px; -moz-border-radius: 10px; background-color: #EFECE7;">
    <div align="right" style="border: 0px solid;">
        <a href="@Url.Action("Index", "AdminHome")" >
            <img src="~/ImagesApp/close.png" border="0" /></a>
    </div>


    <table style="width: 100%">
        <!-- NAZIV EKRANA-->
        <tr>
            <td align="left" colspan="2">
                <font size="5"><b>@Html.Translate("KONFIGURATOR")</b></font>
            </td>
            <td align="right" rowspan="2">
                <img src="~/ImagesApp/KonfiguratorIkona.png" />
            </td>
        </tr>
        <tr>
            <td align="left" colspan="2">
                <font size="4"><b>6.</b> @Html.Translate("Brojevi polja.")</font>
            </td>

        </tr>
        <tr>
            <td align="left" colspan="3">
                <hr />
            </td>
        </tr>
        <tr>
            <td align="left" colspan="3">
                <font> @Html.Translate("Pridružite brojeve pločama.")</font>
            </td>
        </tr>
       
        <!-- TABELA:  -->
        <tr>
            <td colspan="3">
                <div id="teren2" style="margin:20px 0 20px 0">
                    @Html.Partial("teren6")
                </div>
                <table style="width:100%;">
                    <tr>
                        <!-- NATRAG -->
                        <td>
                            <input type="button" value='@Html.Translate("BTN:BACK").ToUpper()' onclick="location.href = '/Configurator/Start5'" class="cancel"/>
                        </td>
                        <!-- OK -->
                        <td style="text-align:right;">
                            @if (Model.SpeedCourtAndTrack == true)
                            {
                                <input type="button" value='@Html.Translate("Spremi").ToUpper()' onclick="location.href = '/Configurator/Start23'" class="config-complete"/>
                            } 
                            else 
                            {
                                <input type="button" value='@Html.Translate("Konfiguration abschliessen").ToUpper()' onclick="location.href = '/Configurator/Start1'" class="config-complete"/>
                            }
                        </td>
                    </tr>
                </table>

            </td>
        </tr>
    </table>

</div>


   <script src="~/Scripts/terenfieldsname.js"></script>




